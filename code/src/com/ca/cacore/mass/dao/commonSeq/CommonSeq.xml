<sqlMap namespace="Seq"> 
	<!-- 查询CRUD信息 -->
	<select id="queryNextVal"  parameterClass="java.lang.String" resultClass="java.lang.String">
		SELECT 
			$seq_name$.NEXTVAL
		FROM DUAL
	</select>
	
</sqlMap>

